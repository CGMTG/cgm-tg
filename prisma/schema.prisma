// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int    @id
  name  String
  token String
}

model Group {
  id          Int     @id
  name        String
  add_by      Int
  username    String?
  permissions Json
  status      String
  admins      Int[]
}
